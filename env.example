# React App Environment Variables
# Copy this file to .env and update the values as needed

# App Configuration
REACT_APP_NAME=Chitransh's Portfolio
REACT_APP_VERSION=1.0.0

# API Configuration (if you add backend later)
REACT_APP_API_URL=https://your-api-url.com
REACT_APP_API_KEY=your-api-key-here

# Analytics (if you add Google Analytics later)
REACT_APP_GA_TRACKING_ID=your-ga-tracking-id

# Contact Form (if you add email service later)
REACT_APP_EMAIL_SERVICE=your-email-service
REACT_APP_EMAIL_TEMPLATE_ID=your-template-id

# Social Media Links
REACT_APP_GITHUB_URL=https://github.com/chitransh-usar
REACT_APP_LINKEDIN_URL=https://www.linkedin.com/in/chitranshk/
REACT_APP_TWITTER_URL=https://twitter.com/your-twitter

# Development Configuration
REACT_APP_DEV_MODE=true
REACT_APP_DEBUG_MODE=false

# Note: All environment variables in React must start with REACT_APP_
# Variables without REACT_APP_ prefix will not be available in your app 